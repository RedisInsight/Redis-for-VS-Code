{
  "name": "redisinsight-vsc-plugin",
  "productName": "RedisInsight VSCode plugin",
  "version": "0.0.1",
  "private": true,
  "description": "RedisInsight Visual Studio Code plugin",
  "main": "dist/extension.js",
  "l10n": "./l10n",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ri-sidebar-view",
          "title": "RedisInsight",
          "icon": "src/media/checklist.svg"
        }
      ]
    },
    "views": {
      "ri-sidebar-view": [
        {
          "type": "webview",
          "id": "ri-sidebar",
          "name": "RedisInsight",
          "icon": "src/media/checklist.svg",
          "contextualTitle": "RedisInsight"
        }
      ]
    },
    "commands": [
      {
        "command": "RedisInsight.openPage",
        "title": "Open RedisInsight",
        "category": "RedisInsight"
      }
    ]
  },
  "resolutions": {
    "@microsoft/fast-element": "^1.12.0",
    "@microsoft/fast-foundation": "^2.49.0",
    "@microsoft/fast-react-wrapper": "^0.3.18"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "postinstall": "patch-package",
    "build": "vite build",
    "dev:sidebar": "RI_DATA_ROUTE=tree vite dev",
    "dev:webview": "RI_DATA_ROUTE=view1 vite dev",
    "l10n:collect": "npx @vscode/l10n-dev export -o ./l10n ./src",
    "storybook:dev": "storybook dev -p 8081 --no-open",
    "storybook:build": "storybook build",
    "watch": "tsc -watch -p ./",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "test:watch": "vitest watch --dir ./src/webviews/src",
    "test:watch:ui": "vitest watch --ui --dir ./src/webviews/src",
    "test:cov": "vitest run --silent --coverage ./src/webviews/src",
    "package:prod": "vsce package --yarn",
    "package:stage": "vsce package --pre-release --yarn",
    "package:stage:mac": "vsce package --pre-release  --yarn --target darwin-x64",
    "package:stage:win": "vsce package --pre-release  --yarn --target win32-x64",
    "package:stage:linux": "vsce package --pre-release  --yarn --target linux-x64",
    "prepare": "husky install"
  },
  "msw": {
    "workerDirectory": "./src/webviews/test/public"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn lint"
  },
  "author": {
    "name": "Redis Ltd.",
    "email": "support@redis.com",
    "url": "https://redis.com/redis-enterprise/redis-insight"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedisInsight/RedisInsight-VSCode-extension.git"
  },
  "bugs": {
    "url": "https://github.com/RedisInsight/RedisInsight-VSCode-extension/issues"
  },
  "keywords": [
    "redisinsight",
    "redis",
    "vsc",
    "visual studio code",
    "plugin",
    "react",
    "nest",
    "typescript",
    "vite"
  ],
  "homepage": "https://github.com/RedisInsight/RedisInsight-VSCode-extension#readme",
