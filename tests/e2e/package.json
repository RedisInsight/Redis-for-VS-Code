{
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "compile": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "install:vs-code": "extest get-vscode",
    "install:chrome-driver": "extest get-chromedriver",
    "install:deps": "yarn run install:vs-code && yarn run install:chrome-driver",
    "install:extension": "extest install-vsix -f redisinsight-vsc-plugin-0.0.1.vsix -e ./test-extensions",
    "test": "yarn run compile && extest run-tests dist/setup.js dist/tests/**/*.js --code_settings settings.json -r . --extensions_dir ./test-extensions"
  },
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/chance": "^1.1.6",
    "@types/clone-deep": "^4.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.15.0",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.8",
    "@types/vscode": "^1.83.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "chai": "^4.3.10",
    "chance": "1.1.8",
    "eslint": "^7.5.0",
    "mocha": "^10.2.0",
    "redis": "^4.6.11",
    "supertest": "^4.0.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.1.3",
    "vscode-extension-tester": "^6.0.0"
  },
  "dependencies": {
    "clone-deep": "^4.0.1",
    "fs-extra": "^11.1.1"
  }
}
