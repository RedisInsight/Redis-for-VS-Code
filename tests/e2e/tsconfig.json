{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "src",
    "removeComments": true,
    "baseUrl": "./",
    "paths": {
      "@e2eSrc/*": ["src/*"]
    }
  },
  "exclude": [
    "./node_modules",
    "./static.ts",
    ".vscode-test",
    "test-resources",
    "test-extensions",
    "**/node_modules"
  ],
}
